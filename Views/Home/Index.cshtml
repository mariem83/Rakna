@{
    ViewData["Title"] = "Home Page";
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/bootstrap.css">
    <link rel="stylesheet" href="css/index.css">
    <link rel="icon" href="img/2222.png">
    <title>Rakna</title>
</head>
<body>

<header class="text-white">

    <nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top opacity-75 " aria-label="Ninth navbar example">
        <div class="container-xl">
            <a asp-area="" asp-controller="Home" asp-action="index" class="navbar-brand">
                <img class="img-fluid brand w-25" src="img/2222.png" alt="">
            </a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarsExample07XL" aria-controls="navbarsExample07XL" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>

            <div class="collapse navbar-collapse navBar1" id="navbarsExample07XL">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item ">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="ourApp">Our App</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Support">Support</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="AboutUs">About us</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-area="" asp-controller="Home" asp-action="Pricing">Pricing</a>
                    </li>
                    <li class="nav-item">
                        <partial class="nav-link" name="_LoginPartial"/>
                    </li>
                </ul>


            </div>
        </div>
    </nav>


    <div class="container text-center textHead">

        <p class="h1">
            PARKING JUST GOT A LOT SIMPLER
        </p>
        <p>Book the Best Spaces</p>


        <a class="m-3" href="http://">
            <img src="img/aaaaaaa.png" alt="">
        </a>
        <a href="http://">
            <img src="img/bbbbbbbbbbbbbb.jpg" alt="">
        </a>


    </div>
</header>


<!--Car-->

<section class="firstSection text-center text-lg-start pt-5 ps-5">
    <div class="container">


        <div class="d-lg-flex d-sm-block justify-content-center align-items-center ">


            <div class="Drive ">
                <p class="fs-1 fw-bold lh-1">
                    DRIVE <br>
                    <span class="m-5 text-primary ">ARRIVE</span> <br>
                    & PARK
                </p>
                <p class="fw-bold fs-6 text-secondary">
                    Enter easily with your mobile parking pass <br>

                    Your space is waiting - pull in and go do your thing
                </p>
            </div>

            <div class="">
                <img class="w-100" src="img/photo_2022-12-21_18-18-57.png" alt="">
            </div>

        </div>
    </div>
</section>


<!--Location car-->


<section class="scoundSection text-center text-lg-start">
    <div class="container">


        <div class="d-lg-flex d-sm-block justify-content-center align-items-center">

            <div class="me-5">
                <img class="w-100 mb-5" src="img/cars2.png" alt="">
            </div>

            <div class="Phone ">
                <p class="fs-1 fw-bold lh-1">
                    DISCOVER
                    <br>
                    <span class="m-5 text-primary ">AMAZING</span> <br>
                    & SPACES
                </p>
                <p class="fw-bold fs-6 text-secondary">
                    Find parking anywhere, for now or for later <br>

                    Compare prices & pick the place that’s best for you
                </p>
            </div>


        </div>
    </div>
</section>

<!--Phone-->

<section class="ThirdSection text-center text-lg-start pt-5">
    <div class="container">


        <div class="d-lg-flex d-sm-block justify-content-center align-items-center">


            <div class="Drive ">
                <p class="fs-1 fw-bold lh-1">
                    RESERVE <br>
                    <span class="m-5 text-primary ">PREPAY </span> <br>
                    & SAVE
                </p>
                <p class="fw-bold fs-6 text-secondary">
                    Enter easily with your mobile parking pass <br>

                    Your space is waiting – pull in and go do your thing
                </p>
            </div>

            <div class="me-5">
                <img class="w-sm-50" src="img/how-it-works-reserve-prepay-save-bp.png" alt="">
            </div>
        </div>
    </div>
</section>


<footer class="d-flex flex-wrap justify-content-between align-items-center py-3 my-4 border-top container">
    <p class="col-md-4 mb-0 text-muted">&copy; 2023 Company, Inc</p>

    <a asp-area="" asp-controller="Home" asp-action="index" class="col-md-4 d-flex align-items-center justify-content-center mb-3 mb-md-0 me-md-auto link-dark text-decoration-none">
        <img class="w-50" src="img/Blue Simple Car Wash Logo (1).png" alt="">
    </a>

    <ul class="nav col-md-4 justify-content-end">
        <li class="nav-item">
            <a asp-area="" asp-controller="Home" asp-action="index" class="nav-link px-2 text-muted">Home</a>
        </li>
        <li class="nav-item">
            <a asp-area="" asp-controller="Home" asp-action="ourApp" class="nav-link px-2 text-muted">Our App</a>
        </li>
        <li class="nav-item">
            <a asp-area="" asp-controller="Home" asp-action="Pricing" class="nav-link px-2 text-muted">Pricing</a>
        </li>
        <li class="nav-item">
            <a asp-area="" asp-controller="Home" asp-action="Support" class="nav-link px-2 text-muted">Support</a>
        </li>
        <li class="nav-item">
            <a asp-area="" asp-controller="Home" asp-action="AboutUs" class="nav-link px-2 text-muted">About us</a>
        </li>
    </ul>
</footer>


<script src="js/bootstrap.bundle.js"></script>
<script src="js/HomePage.js"></script>

@section Scripts {
    <partial name="_ValidationScriptsPartial"/>
}

</body>
</html>